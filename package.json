{
  "name": "ui",
  "version": "0.0.1",
  "private": true,
  "license": "MIT",
  "type": "module",
  "author": {
    "name": "buildy",
    "url": "https://buildy.tw"
  },
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "install:web": "bun install --cwd apps/web",
    "dev:web": "bun run --cwd apps/web dev",
    "build:web": "bun run --cwd apps/web build",
    "install:cab": "bun install --cwd apps/cab",
    "dev:cab": "bun run --cwd apps/cab dev",
    "build:cab": "bun run --cwd apps/cab build",
    "install:dash": "bun install --cwd apps/dash",
    "dev:dash": "bun run --cwd apps/dash dev",
    "build:dash": "bun run --cwd apps/dash build",
    "build": "bun run --cwd packages/cli build",
    "build:cli": "bun run --cwd packages/cli build",
    "dev:cli": "bun run --cwd packages/cli dev",
    "type-check": "tsc --build tsconfig.build.json",
    "type-check:watch": "tsc --build tsconfig.build.json --watch",
    "clean": "tsc --build tsconfig.build.json --clean",
    "registry:utility": "bun run --cwd packages/ui8kit build:utility",
    "registry:semantic": "bun run --cwd packages/ui8kit build:semantic",
    "start:brain": "bun run --cwd packages/@ui8kit/brain start",
    "build:chat": "bun run --cwd packages/@ui8kit/chat build",
    "dev:chat": "bun run --cwd packages/@ui8kit/chat dev",
    "test:chat": "bun run --cwd packages/@ui8kit/chat test",
    "lint:chat": "bun run --cwd packages/@ui8kit/chat lint",
    "storybook:chat": "bun run --cwd packages/@ui8kit/chat storybook"
  },
  "devDependencies": {
    "@babel/generator": "^7.27.1",
    "@babel/parser": "^7.27.1",
    "@babel/traverse": "^7.27.1",
    "@babel/types": "^7.27.1",
    "@eslint/js": "^9.32.0",
    "@types/node": "^20.8.0",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.38.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "glob": "^11.0.3",
    "tsx": "^4.20.3",
    "typescript": "^5.9.2"
  },
  "dependencies": {
    "@types/natural": "^6.0.1",
    "eslint": "^9.32.0",
    "jsdom": "^26.1.0",
    "levenshtein": "^1.0.5",
    "natural": "^8.1.0",
    "porter-stemmer": "^0.9.1"
  }
}
