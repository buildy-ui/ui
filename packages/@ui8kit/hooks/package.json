{
  "name": "@ui8kit/hooks",
  "version": "0.1.0",
  "description": "A collection of hooks for state and UI management for buildy/ui",
  "homepage": "https://buildy.tw",
  "license": "MIT",
  "author": "buildy-ui",
  "keywords": [
    "frontend",
    "hooks",
    "library",
    "tailwind",
    "utility-first",
    "semantic",
    "react",
    "react-hooks",
    "state"
  ],
  "sideEffects": false,
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "repository": {
    "url": "https://github.com/buildy-ui/ui.git",
    "type": "git",
    "directory": "packages/@ui8kit/hooks"
  },
  "peerDependencies": {
    "react": "^19.1.0",
    "react-dom": "^19.1.0"
  }
}
