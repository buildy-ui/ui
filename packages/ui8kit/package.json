{
  "name": "@buildy-ui/ui8kit",
  "version": "1.0.1",
  "description": "BuildY UI8Kit - Open source React UI components library with Tailwind utility-first and semantic classes",
  "main": "src/index.ts",
  "workspaces": [
    ".workspaces/*"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "build:utility": "bunx buildy-ui@latest build --output ./@registry/r/utility",
    "build:semantic": "bunx buildy-ui@latest build --output ./@registry/r/semantic ./semantic/registry.json"
  },
  "dependencies": {
    "react": "^19.1.0",
    "react-dom": "^19.1.0"
  }
}
